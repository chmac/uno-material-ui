{
  "name": "uno-material-ui",
  "version": "1.0.1",
  "description": "Components and extensions for Material UI",
  "main": "dist/index.js",
  "scripts": {
    "build:docs": "parcel build ./srcdocs/index.html --no-source-maps -d ./docs",
    "start:docs": "parcel ./srcdocs/index.html --no-source-maps --port 8080 --open -d ./docs",
    "test": "jest",
    "test-export": "node ./test",
    "build": "tsc",
    "lint": "tslint src/**/*.ts src/**/*.tsx"
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setupTest.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/uno-material-ui.git"
  },
  "keywords": [
    "material-ui",
    "react",
    "hooks",
    "components"
  ],
  "author": "Unosquare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unosquare/uno-material-ui/issues"
  },
  "homepage": "https://github.com/unosquare/uno-material-ui#readme",
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.12",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.8.0",
    "react-highlight": "^0.12.0",
    "ts-jest": "^24.0.2",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-easy": "^1.0.2",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.2",
    "jss": "^10.0.0-alpha.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-form-validator-core": "^0.6.3",
    "react-router-dom": "^5.0.1",
    "uno-react": "^0.7.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "files": [
    "dist"
  ],
  "parcelCleanPaths": [
    "docs/*.*",
    "distsample/*.*"
  ]
}

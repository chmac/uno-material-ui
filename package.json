{
  "name": "uno-material-ui",
  "version": "0.1.3",
  "description": "Components and extensions for Material UI",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-export": "node ./test",
    "build": "tsc",
    "lint": "tslint src/**/*.ts src/**/*.tsx"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/test/setupTest.js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.(js|jsx)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/uno-material-ui.git"
  },
  "keywords": [
    "material-ui",
    "react",
    "hooks",
    "components"
  ],
  "author": "Unosquare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unosquare/uno-material-ui/issues"
  },
  "homepage": "https://github.com/unosquare/uno-material-ui#readme",
  "devDependencies": {
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "jss": "^10.0.0-alpha.9",
    "react-form-validator-core": "^0.6.2",
    "uno-react": "^0.3.4"
  },
  "files": [
    "dist"
  ]
}
